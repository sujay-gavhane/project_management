<%= link_to 'New Project', new_project_path, class: 'pull-right btn btn-primary margin-bottom-30' %>
<div class="row">
	<div class="">
		<table class="table table-bordered">
			<thead>
				<tr>
					<th>
						Title
					</th>
					<th>
						Action
					</th>
				</tr>
			</thead>
			<tbody>
				<% @projects.each do |project| %>
					<tr>
						<td>
							<%= project.title %>
						</td>
						<td>
							<%= link_to 'Edit Project', edit_project_path(project) %>
						</td>
					</tr>
				<% end %>
			</tbody>
		</table>
	</div>
</div>